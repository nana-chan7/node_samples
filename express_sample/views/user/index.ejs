<h1>ユーザホーム</h1>

<dl>
    <dt>氏名</dt>
    <!-- ASPタグ -->
        <dd>
            <%= user.name %>
        </dd>

    <dt>E-mail</dt>
        <dd>
            <%= user.email %>
        </dd>

    <dt>趣味</dt>
        <dd>
            <ul>      
                <!-- user.hobby を繰り返し liタグで表示 -->
                <% user.hobby.forEach((value)=> { %>
                    <li>
                        <%= value %>
                    </li>
                <% }) %>
            </ul>
        </dd>
</dl>

<div class="mt-3">
    <a class="btn btn-outline-primary" href="/user/logout">ログアウト</a>
</div>

